{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "setupFiles": [
    "<rootDir>/setup-env.ts"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(@sigfarm/auth-guard-nest|@sigfarm/auth-contracts|jose)/)"
  ],
  "maxWorkers": 1
}
